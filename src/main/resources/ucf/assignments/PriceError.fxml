            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="74.0" layoutX="14.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@venti.jpg" />
         </image>
      </ImageView>
      <Label alignment="TOP_LEFT" layoutX="98.0" layoutY="43.0" prefHeight="40.0" prefWidth="186.0" text="&quot;There seems to be an issue with your item's price!&quot;" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="ErrorOKButton" layoutX="106.0" layoutY="157.0" mnemonicParsing="false" onAction="#ErrorOKClicked" prefHeight="26.0" prefWidth="88.0" text="OK" />
